{
	"manifest_version": 3,
	"name": "WeCook Extension",
	"version": "1.0",
	"description": "Import meals from WeCook",
	"permissions": ["activeTab", "storage"],
	"host_permissions": ["https://wecook-backend.onrender.com/*"],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
