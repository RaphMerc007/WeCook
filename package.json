{
	"name": "wecook",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"web-app",
		"backend"
	],
	"scripts": {
		"build": "cd web-app && yarn build && cd ../backend && yarn build",
		"start": "cd backend && NODE_ENV=production yarn start",
		"dev": "concurrently \"cd web-app && yarn dev\" \"cd backend && yarn dev\""
	},
	"devDependencies": {
		"concurrently": "^8.2.2"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
